/misc		emmc	/dev/misc		flags=display="Misc";backup=1
/data		ext4	/dev/block/mmcblk0p7
/system		ext4	/dev/block/mmcblk0p5
/cache		ext4	/dev/block/mmcblk0p6	flags=backup=0;usermrf;
/boot		emmc	/dev/bootimg
/recovery	emmc	/dev/recovery
/sdcard2	vfat	/dev/block/mmcblk1p1	flags=display="Micro SDCard";storage;wipeingui;removable;backup=0
/nvram		emmc	/dev/nvram		flags=display="IMEI/NVRAM";backup=1;backupname="IMEI/NVRAM"
/usb-otg	vfat	/dev/block/sda1		flags=display="USB-OTG";storage;removable;backup=0
